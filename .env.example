# Mattermost 공통 환경변수
# 이 파일을 .env로 복사하여 사용하세요

# Mattermost 서버 URL (필수)
MM_BASE_URL=https://meeting.ssafy.com/

# 방법 1: Session Cookie 직접 입력 (권장)
MM_SESSION_COOKIE=MMAUTHTOKEN=your_token_here; MMCSRF=your_csrf_here

# 방법 2: 개별 토큰 입력 (자동으로 Session Cookie로 조합됨)
# MM_SESSION_COOKIE가 비어있을 때만 아래 값들이 자동 조합됩니다
MM_MMAUTHTOKEN=onHere
MM_MMCSRF=onHere

# 기본 포스트 ID (선택)
MM_POST_ID=

